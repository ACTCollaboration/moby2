export MOBY2_PREFIX=/home/lmaurin/code/build

export LDFLAGS="-L$MOBY2_PREFIX/lib $LDFLAGS"
export CFLAGS="-I$MOBY2_PREFIX/include $CFLAGS"
export CPPFLAGS="-I$MOBY2_PREFIX/include $CPPFLAGS"

export PATH=$MOBY2_PREFIX/bin:/home/lmaurin/code/anaconda/bin:${PATH}
export PYTHONPATH=$PYTHONPATH:$MOBY2_PREFIX/lib64/python2.7/site-packages:$MOBY2_PREFIX/lib/python2.7/site-packages:/home/lmauin/code/anaconda/lib/python2.7/site-packages:/home/lmaurin/code/anaconda/lib64/python2.7/site-packages

export LD_LIBRARY_PATH=${MOBY2_PREFIX}/lib64:${MOBY2_PREFIX}/lib:$LD_LIBRARY_PATH
