export MOBY2_PREFIX=/home/rdunner/moby2/build

export LDFLAGS="-L$MOBY2_PREFIX/lib $LDFLAGS"
export CFLAGS="-I$MOBY2_PREFIX/include $CFLAGS"
export CPPFLAGS="-I$MOBY2_PREFIX/include $CPPFLAGS"

export PATH=$MOBY2_PREFIX/bin:/home/act/anaconda/bin:${PATH}
export PYTHONPATH=$PYTHONPATH:$MOBY2_PREFIX/lib64/python2.7/site-packages:$MOBY2_PREFIX/lib/python2.7/site-packages:/home/act/anaconda/lib/python2.7/site-packages:/home/act/anaconda/lib64/python/site-packages

export LD_LIBRARY_PATH=${MOBY2_PREFIX}/lib64:${MOBY2_PREFIX}/lib:$LD_LIBRARY_PATH
